        },
        "linux-x64": {
          "fileName": "xpack-arm-none-eabi-gcc-10.3.1-2.3-linux-x64.tar.gz",
          "sha256": "559dcf1c2dfddac513110fe23da0ef254032c09967eaa901f075515d51818719"
        },
        "win32-ia32": {
          "fileName": "xpack-arm-none-eabi-gcc-10.3.1-2.3-win32-ia32.zip",
          "sha256": "c98fc443d7ef3524d42a40b6635e8b99c736efac77729f8a70463bb9ba19bc85"
