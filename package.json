        },
        "linux-ia32": {
          "fileName": "xpack-arm-none-eabi-gcc-10.2.1-1.1-linux-ia32.tar.gz",
          "sha256": "8adaca8af367a54d1acd2b274cfdc18384b87fbe0ba613322ea3ead20677a132"
        },
        "linux-x64": {
          "fileName": "xpack-arm-none-eabi-gcc-10.2.1-1.1-linux-x64.tar.gz",
          "sha256": "840e90b527d30143b8d8e5eb894218858ed14226bdc8664686f0848592b6e474"
