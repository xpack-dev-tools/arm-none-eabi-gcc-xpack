  "description": "A binary xPack with the GNU Arm Embedded GCC toolchain executables",
  "main": "",
  "scripts": {
    "npm-install": "npm install",
    "pack": "npm pack",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "test": "echo \"Error: no test specified\" && exit 1",
    "liquidjs": "liquidjs"
  },
  "repository": {
