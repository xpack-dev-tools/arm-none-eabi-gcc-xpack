        },
        "linux-ia32": {
          "fileName": "xpack-arm-none-eabi-gcc-10.3.1-2.1-linux-ia32.tar.gz",
          "sha256": "a53a7a371dfd2385a3c67eceb4875e99ccaa8b2c6f29942fb806b597ca07b1d6"
        },
        "linux-x64": {
          "fileName": "xpack-arm-none-eabi-gcc-10.3.1-2.1-linux-x64.tar.gz",
          "sha256": "eb4c7cb16ac1ab7d629ce807847692d5add9f13f23a3196fc545acc6f410a8cf"
